    <!-- Content page -->
    <section class="bg0 p-t-104 p-b-116">
      <div class="container" id="loginShow">
        <form action="/login" method="post">
          <h1>Login Form</h1>
          <br />

          <%if(locals.msg){%>
          <div class="alert alert-warning alert-dismissible fade show">
            <strong>Warning!</strong> <%= msg%>
            <button type="button" class="close" data-dismiss="alert">
              &times;
            </button>
          </div>
          <%}%>

          <div class="form-wrapper">
            <label>UserName</label>
            <input
              type="text"
              name="Lusername"
              placeholder="Place your User name"
              class="form-control"
              id="LusernameId"
              required
            />

            <% if(locals.err){%> <% if(locals.err.Susername){%>
            <span style="color: crimson;"><br /><%=err.Susername.msg%> </span
            ><%}}else{%>
            <span id="LUserNameSId" style="color: crimson;"
              ><br />
              Should contain atlesst six alphanumeric characters
            </span>
            <%}%>
          </div>

          <div class="form-wrapper">
            <label>Password</label>
            <input
              type="password"
              name="pass"
              placeholder="Place your Password"
              class="form-control"
              id="LpasswordId"
              required
            />

            <% if(locals.err){%><% if(locals.err.pass){%>
            <span style="color: crimson;"
              ><br />
              <%=err.pass.msg%></span
            >
            <%}}else{%><span id="LPasswordSId" style="color: crimson;"
              ><br />Should contain atlesst six alphanumeric characters
            </span>
            <%}%>
          </div>
          <button
            type="submit"
            class="flex-c-m stext-101 cl0 size-121 bg3 bor1 hov-btn3 p-lr-15 trans-04 pointer"
          >
            Login
          </button>
          <br />
          <span>
            Dont have account?
            <button style="color:rgb(130, 0, 0);" id="clickShowsignup">
              Click Me
            </button>
          </span>
        </form>
        <br />
      </div>
      <!--===============================================================================================-->
      <div class="container" id="hideSignup">
        <form action="/signup" method="post">
          <h1>Registration Form</h1>
