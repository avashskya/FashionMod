    <script src="/vendor/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script>
      $(".js-pscroll").each(function() {
        $(this).css("position", "relative");
        $(this).css("overflow", "hidden");
        var ps = new PerfectScrollbar(this, {
          wheelSpeed: 1,
          scrollingThreshold: 1000,
          wheelPropagation: false
        });

        $(window).on("resize", function() {
          ps.update();
        });
      });

      // Login
      $(document).ready(() => {
        hideerror();
        $("#hideSignup").hide();
        $("#loginShow").show();
      });

      $("#clickShowsignup").click(() => {
        $("#hideSignup").show("blur");
        $("#loginShow").hide("blur");
        document.getElementById("titles").innerHTML = "Registration";
      });
      $("#clickShowlogin").click(() => {
        $("#hideSignup").hide("blur");
        $("#loginShow").show("blur");
        document.getElementById("titles").innerHTML = "Login";
      });

      $("#LusernameId").focusout(() => {
        let value = $("#LusernameId").val();
        const run = /(?!^[0-9]*$)(?!^[a-zA-Z]*$)^([a-zA-Z0-9]{6,16})$/;
        hideerror();
        if (!value.match(run)) {
          $("#LusernameId").focus();
          $("#LUserNameSId").show("slow");
        }
      });
      $("#LpasswordId").focusout(() => {
        let value = $("#LpasswordId").val();
        const run = /(?!^[0-9]*$)(?!^[a-zA-Z]*$)^([a-zA-Z0-9]{6,16})$/;
        hideerror();
        if (!value.match(run)) {
          $("#LpasswordId").focus();
          $("#LPasswordSId").show("slow");
        }
      });

      // Signup
      const hideerror = () => {
        $("#NameSId").hide();
        $("#AddressSID").hide();
        $("#PhoneSId").hide();
        $("#UserNameSId").hide();
        $("#PasswordSId").hide();
        $("#ConPassSId").hide();
        //login hide
        $("#LUserNameSId").hide();
        $("#LPasswordSId").hide();
      };

      $("#NameId").focusout(() => {
        let value = $("#NameId").val();
        const run = /^[a-z A-Z]{3,20}$/;
        hideerror();
        if (!value.match(run)) {
          $("#NameId").focus();
          $("#NameSId").show("slow");
        }
      });

      $("#AddressId").focusout(() => {
        let value = $("#AddressId").val();
        const run = /^[#.0-9a-zA-Z\s,-]{10,30}$/;
        hideerror();
        if (!value.match(run)) {
          $("#AddressId").focus();
          $("#AddressSID").show("slow");
        }
      });

      $("#PhoneId").focusout(() => {
        let value = $("#PhoneId").val();
        const run = /^[0-9]{10,10}$/;
        hideerror();
        if (!value.match(run)) {
          $("#PhoneId").focus();
          $("#PhoneSId").show("slow");
        }
      });

      $("#usernameId").focusout(() => {
        let value = $("#usernameId").val();
        const run = /(?!^[0-9]*$)(?!^[a-zA-Z]*$)^([a-zA-Z0-9]{6,16})$/;
        hideerror();
        if (!value.match(run)) {
          $("#usernameId").focus();
          $("#UserNameSId").show("slow");
        }
      });
      $("#passwordId").focusout(() => {
        let value = $("#passwordId").val();
        const run = /(?!^[0-9]*$)(?!^[a-zA-Z]*$)^([a-zA-Z0-9]{6,16})$/;
        hideerror();
        if (!value.match(run)) {
          $("#passwordId").focus();
          $("#PasswordSId").show("slow");
        }
      });
      $("#ConPassId").focusout(() => {
        let value = $("#ConPassId").val();
        hideerror();
        if (value !== $("#passwordId").val()) {
          $("#ConPassSId").show("slow");
        }
      });
    </script>
