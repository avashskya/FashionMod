<script src="/vendor/sweetalert/sweetalert.min.js"></script>

<script>
  deleteFromCard = id => {
    if (confirm("Do you want to remove the item from the card")) {
      console.log("yess");
      $.ajax({
        url: "/users/addToCard",
        method: "DELETE",
        contentType: "application/json",
        data: JSON.stringify({ id: id }),
        success: (response, status) => {
          if (status == "success") {
            if (response == "Deleated") {
              $("#tableId").load(" #tableId");
              $("#SumTotoalid").load(" #SumTotoalid");
            } else {
              alert(response);
            }
          } else {
          }
        },
      });
    }
  };
  updateCard = id => {
    let qty = $("#QTYNewID" + id).val();
    let price = $("#priceid" + id)
      .text()
      .split(".")
      .pop();
    $.ajax({
      url: "/users/addToCard",
      method: "PUT",
      contentType: "application/json",
      data: JSON.stringify({ id: id, qty: qty, price: price }),
      success: (response, state, xrh) => {
        if (state == "success") {
          if (response == "Updated") {
            $("#tableId").load(" #tableId");
            $("#SumTotoalid").load(" #SumTotoalid");
          } else {
            alert("Error ::" + response);
          }
        } else {
          alert("Please wate Error : 404");
        }
      },
    });
  };
</script>